        showCancelButton: true,
        confirmButtonColor: "#DD6B55",
        confirmButtonText: `Logout`,
        closeOnConfirm: false,
      },
      function (isConfirm) {
        if (!isConfirm) return;
        $.ajax({
          url: `/admin/logout`,
          type: "GET",
          dataType: "html",
          data: {
            status,
          },
          success: function (result) {
            window.location.href = "<%=process.env.ADMIN_BASE_URL%>/logout";
          },
          error: function (xhr, ajaxOptions, thrownError) {
            swal(`Error in logout!`, "Please try again", "error");
          },
        });
